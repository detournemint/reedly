<% if(this.collection.length > 0){ %>
  <% this.collection.forEach(function(feed){ %>
    <div class="feed" id="<%= feed.escape("id") %>">
     <h3 class="feed-title"> 
        <a href="#feeds/<%= feed.escape('id') %>">
          <%= feed.escape('title') %>
        </a> 
      </h3><button class="delete-feed" data-feed-id="<%= feed.escape("id") %>"> Delete Feed</button>
      <ul>
      <% if(feed.entries().length > 0){ %>
        <% feed.entries().each(function(entry){ %>
          <h6><li><%= entry.escape('title') %></li></h6>
        <% }); %>
      <% } else { %>
        <h4> No new entries </h4>
      <% } %>
      </ul>
    </div>
  <% }) %>
<% } else { %>
  <h2> You have no feeds, please add some. </h2>
<% } %>